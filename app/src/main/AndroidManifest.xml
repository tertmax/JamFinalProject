<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.user.jamproject">
    <permission android:name="android.permission.INTERNET"/>
    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:label="Statistics Activity"
            android:name=".StatisticsActivity" >
        </activity>
           <activity
            android:label="Fire department"
            android:name=".FireDescriptionActivity" >
        </activity>
        <activity
            android:label="Police activity"
            android:name=".PoliceDescriptionActivity" >
        </activity>
        <activity
            android:label="Ambulance"
            android:name=".AmbulanceDescriptionActivity" >
        </activity>
        <activity
            android:label="Poison center"
            android:name=".PoisonDescriptionActivity" >
        </activity>
        <activity
            android:label="Helpline"
            android:name=".HelplineDescriptionActivity" >
        </activity>


    </application>

</manifest>
